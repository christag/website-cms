{
  "collectionName": "components_shared_contact_info",
  "info": {
    "displayName": "Contact Info",
    "description": "Reusable contact information component"
  },
  "options": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": false
    },
    "phone": {
      "type": "string",
      "required": false
    },
    "address": {
      "type": "text",
      "required": false
    },
    "timezone": {
      "type": "string",
      "required": false,
      "default": "EST"
    },
    "availability": {
      "type": "text",
      "required": false,
      "description": "Available hours or schedule"
    },
    "preferredContact": {
      "type": "enumeration",
      "enum": [
        "Email",
        "Phone",
        "LinkedIn",
        "Calendar"
      ],
      "default": "Email"
    }
  }
} 